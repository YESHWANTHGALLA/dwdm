x <- c(4, 1, 5, 7, 10, 2, 50, 25, 90, 36)  # Number of mobile phones sold
y <- c(12, 5, 13, 19, 31, 7, 153, 72, 275, 110)  # Money
plot(x, y, 
     main = "Scatter Plot of Mobile Phones Sold vs Money",
     xlab = "Number of Mobile Phones Sold",
     ylab = "Money",
     pch = 19,  # Point character (solid circles)
     col = "blue")  # Color of the points
abline(lm(y ~ x), col = "red")
